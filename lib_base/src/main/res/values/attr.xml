<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 主要文字颜色 -->
    <attr name="textColorPrimary" format="color" />
    <attr name="textColorPrimaryInverse" format="color|reference" />

    <!-- 次要文本颜色 -->
    <attr name="textColorSecondary" format="color" />

    <!-- 高亮文字颜色 -->
    <attr name="textColorHighlight" format="color" />

    <!-- 提示文字颜色 -->
    <attr name="textColorHint" format="color" />

    <!-- 时间文字颜色-->
    <attr name="textColorCalendar" format="color" />

    <!-- 分线隔颜色 -->
    <attr name="dividerColor" format="color" />

    <!-- 背景颜色 -->
    <attr name="backgroundColor" format="color" />
    <attr name="backgroundColorSecondary" format="color" />
    <declare-styleable name="RoundImageView">
        <attr name="type" format="enum">
            <enum name="circle" value="1" />
            <enum name="round" value="2" />
        </attr>
        <attr name="radius" format="dimension" />ssss
    </declare-styleable>
    <!--    <declare-styleable name="RoundImageView1">-->
    <!--        <attr name="border_thickness" format="dimension" />-->
    <!--        <attr name="border_inside_color" format="color" />-->
    <!--        <attr name="border_outside_color" format="color" />-->
    <!--    </declare-styleable>-->


    <declare-styleable name="MineLabelView">
        <attr name="left_imageview" format="reference|integer" />
        <attr name="label_name" format="string" />

    </declare-styleable>


    <declare-styleable name="SystemSettingLabelView">
        <attr name="left_text" format="string" />
        <attr name="right_text" format="string" />

    </declare-styleable>


    <declare-styleable name="button">
        <attr name="normal_ico" format="reference" />
        <attr name="press_ico" format="reference" />
        <attr name="nmfontcolor" format="color" />
        <attr name="prsfontcolor" format="color" />
        <attr name="normal_conver_drawable" format="reference" />
        <attr name="press_conver_drawable" format="reference" />
    </declare-styleable>


    <declare-styleable name="SwitchButton">
        <attr name="kswThumbDrawable" format="reference" />
        <attr name="kswThumbColor" format="color|reference" />
        <attr name="kswThumbMargin" format="dimension|reference" />
        <attr name="kswThumbMarginTop" format="dimension|reference" />
        <attr name="kswThumbMarginBottom" format="dimension|reference" />
        <attr name="kswThumbMarginLeft" format="dimension|reference" />
        <attr name="kswThumbMarginRight" format="dimension|reference" />
        <attr name="kswThumbWidth" format="dimension|reference" />
        <attr name="kswThumbHeight" format="dimension|reference" />
        <attr name="kswThumbRadius" format="dimension|reference" />
        <attr name="kswBackRadius" format="dimension|reference" />
        <attr name="kswBackDrawable" format="reference" />
        <attr name="kswBackColor" format="color|reference" />
        <attr name="kswFadeBack" format="boolean" />
        <attr name="kswBackMeasureRatio" format="float" />
        <attr name="kswAnimationDuration" format="integer" />
        <attr name="kswTintColor" format="color|reference" />
        <attr name="kswTextOn" format="string" />
        <attr name="kswTextOff" format="string" />
        <attr name="kswTextMarginH" format="dimension" />
        <attr name="kswAutoAdjustTextPosition" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Battery">
        <attr name="batteryOrientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="batteryColor" format="color" />
        <attr name="batteryPower" format="integer" />
    </declare-styleable>

    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer" />
        <attr name="stroke_width" format="dimension" />
        <attr name="color_tick" format="color" />
        <attr name="color_checked" format="color" />
        <attr name="color_unchecked" format="color" />
        <attr name="color_unchecked_stroke" format="color" />
    </declare-styleable>

    <declare-styleable name="CropImageView">
        <attr name="cropMaskColor" format="color" />
        <attr name="cropBorderColor" format="color" />
        <attr name="cropBorderWidth" format="dimension" />
        <attr name="cropFocusWidth" format="dimension" />
        <attr name="cropFocusHeight" format="dimension" />
        <attr name="cropStyle" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="circle" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>


    <declare-styleable name="ArrowSettingLabelView">
        <attr name="label_arrow_text" format="string" />
        <attr name="label_arrow_dividing_line" format="boolean" />

    </declare-styleable>

    <!--多状态布局-->
    <declare-styleable name="StateLayout">
        <attr name="failure_res" format="reference" />
        <attr name="empty_res" format="reference" />
        <attr name="loading_res" format="reference" />
        <attr name="offline_res" format="reference" />
    </declare-styleable>

    <attr name="empty_image_src" format="reference" />
    <attr name="failure_image_src" format="reference" />
    <attr name="offline_image_src" format="reference" />
    <!--多状态布局-->

</resources>